---
layout: null
---
[
  {% for post in site.categories.faqs %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.categories | join: ', ' }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}",
      "app"      : "{{ post.app }}",
      "order"    : "{{ post.order }}",
      "content"  : "{{ post.content | strip_newlines }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
